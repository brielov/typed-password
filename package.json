{
  "name": "typed-password",
  "version": "2.0.0",
  "description": "A simple rule-based typed add-on for checking for password strength",
  "main": "dist/typed-password.js",
  "module": "dist/typed-password.mjs",
  "typings": "dist/typed-password.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "postinstall": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "test": "jest"
  },
  "keywords": [
    "check",
    "match",
    "password",
    "rules",
    "typed",
    "validation",
    "validator"
  ],
  "license": "MIT",
  "author": "Gabriel Vaquer <brielov@icloud.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brielov/typed-password.git"
  },
  "bugs": {
    "url": "https://github.com/brielov/typed-password/issues"
  },
  "homepage": "https://github.com/brielov/typed-password#readme",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "esbuild": "^0.14.9",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.4",
    "pinst": "^2.1.6",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.62.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "rsts": "^1.0.0",
    "typed": "^2.1.0"
  }
}
